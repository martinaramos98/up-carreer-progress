    "db:studio": "deno run --node-modules-dir --allow-env --allow-read --allow-sys --allow-write --allow-run npm:drizzle-kit studio"
  },
  "imports": {
    "@libsql/client": "npm:@libsql/client@^0.14.0",
    "@std/assert": "jsr:@std/assert@1",
    "@types/express": "npm:@types/express@^5.0.0",
    "drizzle-kit": "npm:drizzle-kit@^0.30.5",
    "drizzle-orm": "npm:drizzle-orm@^0.40.0",
    "express": "npm:express@^4.21.2"
  }
}
